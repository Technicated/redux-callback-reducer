{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "modules": true,
    "jsx": false
  },
  "rules": {
    "strict": 0,
    "max-len": [
      2,
      {
        "code": 120,
        "tabWidth": 4,
        "ignoreUrls": true
      }
    ],
    "no-use-before-define": [
      2,
      {
        "functions": false
      }
    ],
    "indent": [
      2,
      4,
      {
        "SwitchCase": 1,
        "VariableDeclarator": 2
      }
    ],
    "import/no-unresolved": 0
  }
}
